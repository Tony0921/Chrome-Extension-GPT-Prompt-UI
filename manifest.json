{
	"manifest_version": 3,
	"name": "GPT Prompt UI",
	"version": "1.3",
	"description": "Chrome extension adds custom UI for enhanced ChatGPT prompts handling.",
	"icons": {
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://chat.openai.com/*"],
			"css": ["content.css"],
			"js": ["content.js"]
			
		}
	],
	"action": {
		"default_title": "GPT Prompt UI"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [
		"tabs"
	]
}